QWidget#m_mainWidget
{
    border: 1px solid #999999;
    border-radius: 0px;
    /*background:#FFFFFF;*/
    border-image:url(:/background/images/background.png) 10 10 10 10;
}

QWidget#m_titleWidget
{
    background:#333333;
}

QWidget#m_choosedImageWidget
{
    border: none;
    border-radius: 0px;
        background-image:url(:/background/images/uploadimage.png);
    background-repeat:no-repeat;
    background-position:center;
