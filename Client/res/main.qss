    border-radius: 5px;
    color:#EEEEEE;
    background:transparent;
    font: 12px;
    /*min-width: 6em;*/
    font-family:微软雅黑;
}

QPushButton
{
    border-radius: 5px;
    color:black;
    background:transparent;
    font: 12px;
    /*min-width: 6em;*/
    padding: 6px;
    font-family:微软雅黑;
}

QPushButton#m_closeBtn
{
color: #999999;
border-radius:1px;
font: bold 9px;
padding: 6px;
background-image:url(:/Title/Button/images/close.png);
background-repeat:no-repeat;
background-position:center;
}
QPushButton#m_closeBtn:hover
{
background:red;
background-image:url(:/Title/Button/images/close.png);
background-repeat:no-repeat;
background-position:center;
}

QPushButton#m_helpBtn
{
color: #999999;
border-radius:1px;
font: bold 9px;
padding: 6px;
background-image:url(:/Title/Button/images/help.png);
background-repeat:no-repeat;
background-position:center;
}
QPushButton#m_helpBtn:hover
{
background:#999999;
background-image:url(:/Title/Button/images/help.png);
background-repeat:no-repeat;
background-position:center;
border-style:solid;
border-width:1px;
border-color:#388BFF;
}


QPushButton#m_chooseImageBtn
{
color:white;
background:transparent;
border-style:solid;
border-width:1px;
border-color:#999999;
}
